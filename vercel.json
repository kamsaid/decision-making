{
    "functions": {
      "src/app/api/**/*": {
        "maxDuration": 300,
        "memory": 3008
      }
    },
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, must-revalidate"
          }
        ]
      }
    ],
    "regions": ["iad1"],
    "git": {
      "deploymentEnabled": {
        "main": true
      }
    }
  }