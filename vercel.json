{
    "functions": {
      "src/app/api/**/*": {
        "maxDuration": 60
      }
    },
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, must-revalidate"
          }
        ]
      }
    ]
  }